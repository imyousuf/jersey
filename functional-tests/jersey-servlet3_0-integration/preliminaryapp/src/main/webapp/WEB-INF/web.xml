    <servlet-mapping>
        <servlet-name>com.sun.jersey.qe.tests.servlet3.preliminaryapp.OneApplication</servlet-name>
        <url-pattern>/oneonly/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.sun.jersey.qe.tests.servlet3.preliminaryapp.TwoApplication</servlet-name>
